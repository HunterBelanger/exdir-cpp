# - Config file for the exdir-cpp package
# It defines the following variables
#  EXDIR_CPP_INCLUDE_DIR - include directory
#  EXDIR_CPP_LIBRARIES    - libraries to link against

get_filename_component(EXDIR_CPP_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(EXDIR_CPP_INCLUDE_DIR "@CONFIG_INCLUDE_DIRS@")

include("${EXDIR_CPP_CMAKE_DIR}/exdir-cpp-targets.cmake")

set(EXDIR_CPP_LIBRARIES "@EXPORT_TARGETS@")
